{
	"name": "@ydb-platform/ydb-slo-action-report",
	"version": "1.0.0",
	"description": "Report action for SLO tests",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"bundle": "bun build ./src/main.ts --target node --format esm --banner '/* eslint-disable */' --outdir dist",
		"format": "bunx --bun prettier **/*.ts --write",
		"lint": "bunx --bun oxlint **/*.ts --fix --node-plugin --import-plugin --security-plugin --disable-react-plugin"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@actions/artifact": "^2.1.1",
		"@actions/core": "^1.10.1",
		"@actions/github": "^6.0.0",
		"canvas": "^2.11.2"
	},
	"devDependencies": {
		"@types/node": "^20.11.19",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	}
}
